{
  "version": 2,
  "name": "celfund",
  "buildCommand": "cd frontend && npm install --legacy-peer-deps --force && CI=false npm run build",
  "outputDirectory": "frontend/build",
  "framework": null,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/server.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/server.py": {
      "runtime": "python3.9"
    }
  },
  "build": {
    "env": {
      "CI": "false",
      "DISABLE_ESLINT_PLUGIN": "true"
    }
  }
}
