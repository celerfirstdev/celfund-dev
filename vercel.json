{
  "version": 2,
  "buildCommand": "cd frontend && npm install --legacy-peer-deps --force && CI=false npm run build",
  "outputDirectory": "frontend/build",
  "installCommand": "echo 'Skipping root install'",
  "framework": null,
  "functions": {
    "backend/server.py": {
      "runtime": "python3.12"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/server.py"
    }
  ]
}
